{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "env": {
        "es6": true
    },
    "rules": {
        "semi": [1, "always"],
        "indent": [1, 4, { "SwitchCase": 1 }],
        "comma-dangle": [0],
        "linebreak-style": "off",
        "curly": [1, "multi"],
        "max-len": [1, 120],
        "class-methods-use-this": "off",
        "no-trailing-spaces": [1, { "skipBlankLines": true }]
    }
}