{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "env": {
        "es6": true
    },
    "rules": {
        "semi": [1, "always"],
        "indent": [1, 4],
        "comma-dangle": [0],
        "linebreak-style":"off",
        "curly": [1, "multi"]
    }
}